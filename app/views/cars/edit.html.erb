<%= form_with url: "/cars/#{@car.id}", method: :patch, local: true do |form| %>
  <%= form.label :make %>
  <%= form.text_field :make %>
  <%= form.label :model %>
  <%= form.text_field :model %>
  <%= form.label :year %>
  <%= form.text_field :year %>
  <%= form.label :auto_pilot %>
  <%= form.text_field :auto_pilot %>
  <%= form.submit "Update Car" %>
<% end %>

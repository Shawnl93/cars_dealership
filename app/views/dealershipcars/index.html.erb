<%= link_to "cars", "/cars" %>
<%= link_to "dealerships", "/dealerships" %>
<%= link_to "New Car", "/dealerships/#{@dealership.id}/cars/new" %>
<%= link_to "Sort", "/dealerships/#{@dealership.id}/cars?sort=1"%>

<% @cars.each do |car| %>
  <p>Make: <%= car.make %></p>
  <p>Model: <%= car.model %> <%= car.model %> <%= link_to "Update #{car.model}", "/cars/#{car.id}/edit", method: :get %></p>
  <p>Year: <%= car.year %></p>
  <p>Auto_pilot: <%= car.auto_pilot %></p>
<% end %>

<%= form_with url: '/dealerships', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :city %>
  <%= form.text_field :city %>
  <%= form.label :charging_stations %>
  <%= form.text_field :charging_stations %>
  <%= form.label :leasing %>
  <%= form.text_field :leasing %>
  <%= form.submit "Create Dealership" %>
<% end %>

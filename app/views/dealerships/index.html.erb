<%= link_to "cars", "/cars" %>
<%= link_to "dealerships", "/dealerships" %>
<%= link_to "New Dealership", "/dealerships/new" %>

<h1> Dealerships index page </h1>
<% @dealerships.created_by.each do |dealership| %>
  <p>Dealership: <%= dealership.name %> <%= link_to "Update #{dealership.name}", "/dealerships/#{dealership.id}/edit", method: :get %> </p>
  <p>Created_at: <%= dealership.created_at %> </p>
<% end %>
